<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <WarnOn>3390;$(WarnOn)</WarnOn>
    </PropertyGroup>

    <ItemGroup>
        <Compile Include="Feliz.Antd.Myriad.Plugins.Typegen.fs" />
    </ItemGroup>

    <ItemGroup>
        <Content Include="build\Feliz.Antd.Myriad.Plugins.Typegen.Test.props">
            <Pack>false</Pack>
            <PackagePath>%(Identity)</PackagePath>
            <Visible>true</Visible>
        </Content>
        
        <Content Include="build\Feliz.Antd.Myriad.Plugins.Typegen.props">
            <Pack>true</Pack>
            <PackagePath>%(Identity)</PackagePath>
            <Visible>true</Visible>
        </Content>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Myriad.Core" Version="0.6.0" PrivateAssets="all" />
    </ItemGroup>

    <Import Project="build/Feliz.Antd.Myriad.Plugins.Typegen.props" />

</Project>
